<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="TV.ModelImpl"
                   namespace="TV.ModelImpl.DbModel">

  <class name="DbPaperFormat" table="PaperFormats">
    <id name="Id" column="id">
      <generator class="guid" />
    </id>
    <property name="Name" column="name" not-null="true" />
    <property name="Width" column="width" not-null="true" />
    <property name="Height" column="height" not-null="true" />
    <property name="IsBuildin" column="isbuildin" not-null="true" />
   </class>
</hibernate-mapping>
