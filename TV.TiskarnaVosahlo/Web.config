  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory name="NHibernate.Test">
      <property name="connection.driver_class">NHibernate.Driver.NpgsqlDriver</property>
      <property name="connection.connection_string">Server=localhost;Database=TiskarnaVosahlo;User ID=njorloj;Password=njorloj;</property>
      <property name="dialect">NHibernate.Dialect.PostgreSQL82Dialect</property>
      <property name="default_schema">public</property>
      <mapping assembly="TV.ModelImpl" />
    </session-factory>
  </hibernate-configuration>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="dotless" path="*.less" verb="GET" type="dotless.Core.LessCssHttpHandler,dotless.Core" resourceType="File" preCondition="" />
    </handlers>
    <modules>
      <add name="ThreadInitializeModule" type="ThreadInitializeModule" />
    </modules>
  </system.webServer>
  <dotless minifyCss="false" cache="true" web="false" strictMath="false" />
</configuration>